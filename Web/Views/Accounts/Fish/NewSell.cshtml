        }

        if (multiSelectVal == "" || multiSelectVal.length <= 0) {
            isFormValid = false;
            $("#spnMainFishNameList").css({ "display": "block"});
        }
        
        if (typeof txtNumAmountPaid === 'undefined' || txtNumAmountPaid == null || txtNumAmountPaid < 0) {
            isFormValid = false;
            $("#txtNumAmountPaid").closest("div").find('span').css("display", "block");
