<script type="text/javascript">

    var cmbFishSellerComboBox;
    var amountPaidByBuyerPicker;
    var dueAmountForBuyerPicker;
    $(document).ready(function () {
        cmbFishSellerComboBox = $("#cmbFishSellerId").data("kendoComboBox");
        amountPaidByBuyerPicker = $("#txtNumAmountPaid").data("kendoNumericTextBox");
        dueAmountForBuyerPicker = $("#txtNumAmountToDue").data("kendoNumericTextBox");
        $("#lstFishSellingLink").addClass("active");
    });


    function OnChangeFishNameList(e) {

        var value = this.value();
        $(".dummy_toggle_fish_controll_area").hide();
        var idList = $.trim($("#dummy_FishIdListString").val());

        for (var i = 0; i < value.length; i++) {
            $("#divfishReportForFishId_" + value[i]).show();
        }

        var removeId = IsFishIdExistInCurrentList();
        if (removeId > 0) {
            $($.trim("#txtNumTotalSellInKG" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtNumPricePerKG" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtNumTotalSellPrice" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtNumPiecesPerKg" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtTotalPiecesFish" + removeId)).val("");
        }

        $("#dummy_FishIdListString").val(value);

        DoAllCalculationCheck();
    }


    function IsFishIdExistInCurrentList() {
        var idList = $.trim($("#dummy_FishIdListString").val());

        var returnFalue = 0;
        if (idList != "") {
            var arrayList = idList.split(",");
            for (index = 0; index < arrayList.length; index++) {
                var moveId = CheckNewChangeExist(arrayList[index]);
                if (moveId > 0) {
                    returnFalue = moveId;
                }
            }
        }
        return returnFalue;
    }

    function CheckNewChangeExist(loopValue) {
        var value = $("#cmbFishNameId").data("kendoMultiSelect").value();
        var returnID = loopValue;
        for (var i = 0; i < value.length; i++) {
            if (value[i] == loopValue) {
                returnID = 0;
            }
        }

        return returnID;
    }


    function DoAllCalculationCheck() {

        var multiSelectVal = $("#cmbFishNameId").data("kendoMultiSelect").value();
        var totalPrice = "";
        
        for (var i = 0; i < multiSelectVal.length; i++) {
            var fishID = multiSelectVal[i];
            if (fishID) {
                var sellInKG = $($.trim("#txtNumTotalSellInKG" + fishID)).data("kendoNumericTextBox").value();
                var pricePerKG = $($.trim("#txtNumPricePerKG" + fishID)).data("kendoNumericTextBox").value();

                if (sellInKG && pricePerKG) {
                    var calCulatedValue = (sellInKG * pricePerKG);
                    $($.trim("#txtNumTotalSellPrice" + fishID)).data("kendoNumericTextBox").value(calCulatedValue);
                }
            }
        }

        DoCalculationTotalSellInKG();
        DoCalculationTotalPriceForAllFish();
        DoCalculationTotalPicesFish();
        OnChangeDoCalculationFishAmountPaid();
    }

    function OnChangeDoCalculationKgAndPricePerKg(e) {
      
        var elementAttr = this.element.context;
        var fishID;
        if (elementAttr && elementAttr.attributes && elementAttr.attributes.fishid) {
            fishID = elementAttr.attributes.fishid.value;
            if (fishID) {
                var sellInKG = $($.trim("#txtNumTotalSellInKG" + fishID)).data("kendoNumericTextBox").value();
