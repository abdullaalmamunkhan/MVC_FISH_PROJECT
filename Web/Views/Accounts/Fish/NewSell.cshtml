    function OnChangeDoCalculationFishAmountPaid() {
        
        var totalPrice = $("#txtNumAllFishCalculationPrice").data("kendoNumericTextBox").value();
        var amountPaid = $("#txtNumAmountPaid").data("kendoNumericTextBox").value();

        if (totalPrice && amountPaid) {
            var calCulatedValue = (totalPrice - amountPaid);
            var toFixedValue = calCulatedValue.toFixed(2);
            if (amountPaid > totalPrice) {
                ShowAjaxNotificationMessage("চমৎকার", "আপনি বিক্রি মূল্যের সব টাকা পরিশোধ করেছেন। আপনাকে ধন্যবাদ।", "success");
                $("#txtNumAmountToDue").data("kendoNumericTextBox").value(toFixedValue);
            } else {
                $("#txtNumAmountToDue").data("kendoNumericTextBox").value(toFixedValue);
            }
        } else {
            $("#txtNumAmountToDue").data("kendoNumericTextBox").value(totalPrice);
        }
    }


