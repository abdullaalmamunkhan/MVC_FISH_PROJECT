    function DoValidationForEveryFish() {
      
        var isThisFishValid = true;
        var multiSelectVal = $("#cmbFishNameId").data("kendoMultiSelect").value();
        for (var i = 0; i < multiSelectVal.length; i++) {
            var fishId = multiSelectVal[i];
            var sellInKG = $($.trim("#txtNumTotalSellInKG" + fishId)).data("kendoNumericTextBox").value();
            var pricePerKG = $($.trim("#txtNumPricePerKG" + fishId)).data("kendoNumericTextBox").value();
            var totalPrice = $($.trim("#txtNumTotalSellPrice" + fishId)).data("kendoNumericTextBox").value();
            var element = $($.trim("#txtNumPiecesPerKg" + fishId));
            var picesOfFish = $($.trim(element.selector)).data("kendoNumericTextBox").value();

            if (typeof sellInKG === 'undefined' || sellInKG == null || sellInKG <= 0) {
                isThisFishValid = false;
                $($.trim("#txtNumTotalSellInKG" + fishId)).closest("div").find('span').css("display", "block");
            }

            if (typeof pricePerKG === 'undefined' || pricePerKG == null || pricePerKG <= 0) {
                isThisFishValid = false;
                $($.trim("#txtNumPricePerKG" + fishId)).closest("div").find('span').css("display", "block");
            }
            if (typeof totalPrice === 'undefined' || totalPrice == null || totalPrice <= 0) {
                isThisFishValid = false;
                $($.trim("#txtNumTotalSellPrice" + fishId)).closest("div").find('span').css("display", "block");
            }
            if (typeof picesOfFish === 'undefined' || picesOfFish == null || picesOfFish <= 0) {
                isThisFishValid = false;
                $($.trim(element.selector)).closest("div").find('span').css("display", "block");
            }
        }

        return isThisFishValid;
    }


    function DoThisFormValidation() {
       
        $(".dummy_fields_error_msg").hide();
