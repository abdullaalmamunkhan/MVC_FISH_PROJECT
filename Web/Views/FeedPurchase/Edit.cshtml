                                <label class="control-label col-md-12">
                                    <span class="ln-bangla">হিসাবের নাম</span> <span class="required" aria-required="true"> * </span>
                                </label>
                                <div class="col-md-12">
                                    @Html.TextBoxFor(m => m.FeedPurcaseTable.SellingReportName, new { @class = "form-control", id = "txtSellingReportName", autocomplete = "off", })
