</div>

<script type="text/javascript">

    $(document).ready(function(){
        $("#lstFeedPurchase").addClass("active");
        ShowActiveDivForMultiSelect();
    });

    function ShowActiveDivForMultiSelect() {

        var stringList = $.trim($("#dummy_AreaIdListString").val());

        var multiArray = stringList.split(',');
        var res = $.merge([], multiArray);
        $("#cmbAreaNameIdList").data("kendoMultiSelect").value(res);

        $(".dummy_toggle_area_controll_area").hide();
        for (var i = 0; i < multiArray.length; i++) {
            $("#divAreaVisibilityByAreaId_" + multiArray[i]).show();
        }
    }


    function LoadFeedCategoryComboBox(thisId,feedLoopID) {

        $.get("/SellOfFeed/GetFeedCategoryListByFeedId", { feedId: thisId }, function (data) {

            var dataSourceGrid = new kendo.data.DataSource({
                data: data,
            });

            $($.trim("#txtCmbFeedCategoryList" + feedLoopID)).kendoComboBox({
                placeholder: "",
                dataTextField: "FeedCategoryName",
                dataValueField: "FeedCategoryId",
                filter: "contains",
                autoBind: false,
                dataSource: dataSourceGrid
            });


        });

    }



    function DoFeedNameListComboBoxChangeEvent(comboBoxVariableName, categoryList,feedLoopID){
       
        comboBoxVariableName.bind("change", function () {
            categoryList.value("");
            if ((comboBoxVariableName.selectedIndex === -1 && comboBoxVariableName.value()) || comboBoxVariableName.value() == "") {
                var feedId = 0;
                LoadFeedCategoryComboBox(feedId,feedLoopID);
            }
            else {
                feedId = comboBoxVariableName.value();
                LoadFeedCategoryComboBox(feedId,feedLoopID);
            }
        });

    }


    function OnChangeAreaNameListMultiSelect(e){
        var value = this.value();
        $(".dummy_toggle_area_controll_area").hide();
        for (var i = 0; i < value.length; i++) {
            $("#divAreaVisibilityByAreaId_" + value[i]).show();
        }
       
        var removeId = IsAreaIdExistInCurrentList();
        if (removeId > 0) {
            $($.trim("#cmbFeedNameList" + removeId)).data("kendoComboBox").value("");
            $($.trim("#txtCmbFeedCategoryList" + removeId)).data("kendoComboBox").value("");
            $($.trim("#txtCmbWeightOfSack" + removeId)).data("kendoComboBox").value("");
            $($.trim("#txtNumSellingFeedSackNumber" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtNumSellingFeedTotalWeight" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtNumSellingFeedPricePerKg" + removeId)).data("kendoNumericTextBox").value("");
            $($.trim("#txtNumSellingFeedTotalPrice" + removeId)).data("kendoNumericTextBox").value("");
        }

        DoCalculationTotalWeight();
        DoCalculateTotalPrice();
       
        $("#dummy_AreaIdListString").val(value);

    }


    function IsAreaIdExistInCurrentList() {
        var idList = $.trim($("#dummy_AreaIdListString").val());
        var returnFalue = 0;
        if (idList != "") {
            var arrayList = idList.split(",");
            for (index = 0; index < arrayList.length; index++) {
                var moveId = CheckNewChangeExist(arrayList[index]);
                if(moveId > 0){
                    returnFalue=moveId;
                }
            }
        }
        return returnFalue;
    }

    function CheckNewChangeExist(loopValue) {
        var value = $("#cmbAreaNameIdList").data("kendoMultiSelect").value();
        var returnID = loopValue;
        for (var i = 0; i < value.length; i++) {
            if (value[i] == loopValue) {
                returnID = 0;
            }
        }
        return returnID;
    }

    function OnChangeSellingFeedWeightPerSack(e){

        var elementAttr = this.element.context;
        var contextID;
        if (elementAttr && elementAttr.attributes && elementAttr.attributes.areaid) {
            contextID = elementAttr.attributes.areaid.value;
            if (contextID) {
                var sackWeight = $($.trim("#txtCmbWeightOfSack" + contextID)).data("kendoComboBox").value();
                var sackNumber = $($.trim("#txtNumSellingFeedSackNumber" + contextID)).data("kendoNumericTextBox").value();
                var calCulatedValue;
                if (sackWeight && sackNumber) {
                    calCulatedValue = (sackNumber * sackWeight);
                    $($.trim("#txtNumSellingFeedTotalWeight" + contextID)).data("kendoNumericTextBox").value(calCulatedValue);
                }


                var totalPrice = $($.trim("#txtNumSellingFeedPricePerKg" + contextID)).data("kendoNumericTextBox").value();
                if (calCulatedValue && totalPrice) {
                    var sackPrice = (totalPrice * calCulatedValue);
                    $($.trim("#txtNumSellingFeedTotalPrice" + contextID)).data("kendoNumericTextBox").value(sackPrice);
                }
            }
        }
        DoCalculationTotalWeight();
        DoCalculateTotalPrice();
    }


    function OnChangeSellingFeedPricePerKg(e){
        var elementAttr = this.element.context;
        var contextID;
        if (elementAttr && elementAttr.attributes && elementAttr.attributes.areaid) {
            contextID = elementAttr.attributes.areaid.value;
            if (contextID) {
                var sackWeight=$($.trim("#txtNumSellingFeedTotalWeight" + contextID)).data("kendoNumericTextBox").value();
                var weightPErKG = $($.trim("#txtNumSellingFeedPricePerKg" + contextID)).data("kendoNumericTextBox").value();

                if (sackWeight && weightPErKG) {
                    var calCulatedValue = (weightPErKG * sackWeight);
                    $($.trim("#txtNumSellingFeedTotalPrice" + contextID)).data("kendoNumericTextBox").value(calCulatedValue);
                }

            }
        }

        DoCalculateTotalPrice();


    }

    function OnChangeSellingFeedTotalPrice(e){

        var elementAttr = this.element.context;
        var contextID;
        if (elementAttr && elementAttr.attributes && elementAttr.attributes.areaid) {
            contextID = elementAttr.attributes.areaid.value;
            if (contextID) {
                var sackWeight=$($.trim("#txtNumSellingFeedTotalWeight" + contextID)).data("kendoNumericTextBox").value();
                var totalPrice = $($.trim("#txtNumSellingFeedTotalPrice" + contextID)).data("kendoNumericTextBox").value();

                if (sackWeight && totalPrice) {
                    var calCulatedValue = (totalPrice / sackWeight);
                    $($.trim("#txtNumSellingFeedPricePerKg" + contextID)).data("kendoNumericTextBox").value(calCulatedValue);
                }
            }
        }

        DoCalculateTotalPrice();
    }


    function DoCalculateTotalPrice(){

        var multiSelectVal = $("#cmbAreaNameIdList").data("kendoMultiSelect").value();
        var totalPrice = "";

        for (var i = 0; i < multiSelectVal.length; i++) {

            var areaID = multiSelectVal[i];
            var sellInKG = $($.trim("#txtNumSellingFeedTotalPrice" + areaID)).data("kendoNumericTextBox").value();
            if (totalPrice == "") {
                totalPrice = sellInKG;
            } else {
                totalPrice = (totalPrice + sellInKG);
            }
        }

        $($.trim("#txtNumSellingFeedTotalPrice")).data("kendoNumericTextBox").value(totalPrice);

        OnChangeDoCalculationDueAmount();
    }



    function OnChangeDoCalculationDueAmount(){
        var totalFeedPrice = $($.trim("#txtNumSellingFeedTotalPrice")).data("kendoNumericTextBox").value();
        var amountPaid = $($.trim("#txtNumAmountPaid")).data("kendoNumericTextBox").value();
    
        if (totalFeedPrice && amountPaid) {
            var calCulatedValue = (totalFeedPrice - amountPaid);
            $($.trim("#txtNumAmountToDue")).data("kendoNumericTextBox").value(calCulatedValue);
            if (amountPaid > totalFeedPrice) {
                ShowAjaxNotificationMessage("চমৎকার", "আপনি সব টাকা পরিশোধ করেছেন। আপনাকে ধন্যবাদ।", "success");
            }
        }else{
            $($.trim("#txtNumAmountToDue")).data("kendoNumericTextBox").value("");
        }
    }


    function DoCalculationTotalWeight(){

        var multiSelectVal = $("#cmbAreaNameIdList").data("kendoMultiSelect").value();
        var totalPrice = "";

        for (var i = 0; i < multiSelectVal.length; i++) {

            var areaID = multiSelectVal[i];
            var sellInKG = $($.trim("#txtNumSellingFeedTotalWeight" + areaID)).data("kendoNumericTextBox").value();
            if (totalPrice == "") {
                totalPrice = sellInKG;
            } else {
                totalPrice = (totalPrice + sellInKG);
            }
        }

        $($.trim("#txtNumSellingFeedTotalWeight")).data("kendoNumericTextBox").value(totalPrice);

    }


    function DoValidationForEveryArea() {

        var isThisFishValid = true;
        var multiSelectVal = $("#cmbAreaNameIdList").data("kendoMultiSelect").value();
        for (var i = 0; i < multiSelectVal.length; i++) {
            var fishId = multiSelectVal[i];

            var feedName = $($.trim("#cmbFeedNameList" + fishId)).data("kendoComboBox").value();
            var feedCatName = $($.trim("#txtCmbFeedCategoryList" + fishId)).data("kendoComboBox").value();
            var sackWeight = $($.trim("#txtCmbWeightOfSack" + fishId)).data("kendoComboBox").value();

            var sackNumber = $($.trim("#txtNumSellingFeedSackNumber" + fishId)).data("kendoNumericTextBox").value();
            var totalWeight = $($.trim("#txtNumSellingFeedTotalWeight" + fishId)).data("kendoNumericTextBox").value();
            var pricePerKG = $($.trim("#txtNumSellingFeedPricePerKg" + fishId)).data("kendoNumericTextBox").value();
            var totalPrice = $($.trim("#txtNumSellingFeedTotalPrice" + fishId)).data("kendoNumericTextBox").value();

            if ($($.trim("#cmbFeedNameList" + fishId)).data("kendoComboBox").selectedIndex === -1 && feedName || feedName == "" || feedName == null) {
                isFormValid = false;
                $($.trim("#cmbFeedNameList" + fishId)).closest("div").find('span').css("display", "block");
            }

            if ($($.trim("#txtCmbFeedCategoryList" + fishId)).data("kendoComboBox").selectedIndex === -1 && feedCatName || feedCatName == "" || feedCatName == null) {
                isFormValid = false;
                $($.trim("#txtCmbFeedCategoryList" + fishId)).closest("div").find('span').css("display", "block");
            }

            if ($($.trim("#txtCmbWeightOfSack" + fishId)).data("kendoComboBox").selectedIndex === -1 && sackWeight || sackWeight == "" || sackWeight == null) {
                isFormValid = false;
                $($.trim("#txtCmbWeightOfSack" + fishId)).closest("div").find('span').css("display", "block");
            }

            if (typeof sackNumber === 'undefined' || sackNumber == null || sackNumber <= 0) {
                isThisFishValid = false;
                $($.trim("#txtNumSellingFeedSackNumber" + fishId)).closest("div").find('span').css("display", "block");
            }

            if (typeof totalWeight === 'undefined' || totalWeight == null || totalWeight <= 0) {
                isThisFishValid = false;
                $($.trim("#txtNumSellingFeedTotalWeight" + fishId)).closest("div").find('span').css("display", "block");
            }
            if (typeof pricePerKG === 'undefined' || pricePerKG == null || pricePerKG <= 0) {
                isThisFishValid = false;
                $($.trim("#txtNumSellingFeedTotalPrice" + fishId)).closest("div").find('span').css("display", "block");
            }

        }

        return isThisFishValid;
    }


    function DoThisFormValidation() {
        $(".dummy_fields_error_msg").hide();
        $("#divExistingEmailMessage,.validation-message-color").hide();

        var isFormValid = true;
        var fishName = $.trim($("#txtSellingReportName").val());
        var reportName = $.trim($("#txtSellingFeedReportNumber").val());
        var sellDate = $("#txtClnSellDate").data("kendoDatePicker").value();
        var multiSelectVal = $("#cmbAreaNameIdList").data("kendoMultiSelect").value();
        var txtNumAmountPaid = $("#txtNumAmountPaid").data("kendoNumericTextBox").value();

        if (sellDate) {
            var dateString = kendo.toString(new Date(sellDate), "MM/dd/yyyy");
            $("#hdnSellDate").val(dateString);
        } else {
            $("#hdnSellDate").val("");
        }
        var dateText = $.trim($("#hdnSellDate").val());

        if (typeof fishName === 'undefined' || fishName == "") {
            isFormValid = false;
            $("#txtSellingReportName").closest("div").find('span').css("display", "block");
        }

        if (typeof reportName === 'undefined' || reportName == "") {
            isFormValid = false;
            $("#txtSellingFeedReportNumber").closest("div").find('span').css("display", "block");
        }

        if (multiSelectVal == "" || multiSelectVal.length <= 0) {
            isFormValid = false;
            $("#spnMainFishNameList").css({ "display": "block"});
        }

        if (typeof txtNumAmountPaid === 'undefined' || txtNumAmountPaid == null || txtNumAmountPaid < 0) {
            isFormValid = false;
            $("#txtNumAmountPaid").closest("div").find('span').css("display", "block");
        }

        if (typeof dateText === 'undefined' || dateText == "") {
            isFormValid = false;
            $("#spnFishSellingDate").css("display", "block");
        }

        if (isFormValid == true) {

            var isReturn = DoValidationForEveryArea();

            if (isReturn == false) {
                ShowAjaxNotificationMessage("দুঃখিত", "দুঃখিত, আপনি নির্ধারিত সব মাছের জন্য হিসাব সম্পূর্ণ করেন নাই!", "warning");
            }

            isFormValid = isReturn;

        }


        if (isFormValid == true) {

            ShowAjaxNotificationMessage("প্রসেসিং", "<img src='" + domainURL + "/images/loading-indicator.gif' draggable='false' class='disable-image' /> ", "success");
            $(".k-button-icon.k-window-action").css({ "display": "none" });

            //DoAllCalculationCheck();
        }


        return isFormValid;;
    }

</script>



