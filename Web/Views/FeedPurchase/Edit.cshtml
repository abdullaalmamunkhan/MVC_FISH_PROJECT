                            </div>

                            <div class="form-group row xs-nopadding-left wow fadeInUp animated">
                                <label class="control-label col-md-12">
                                    <span class="ln-bangla">হিসাবের নং</span> <span class="required" aria-required="true"> * </span>
                                </label>
                                <div class="col-md-12">
                                    @Html.TextBoxFor(m => m.FeedPurcaseTable.SellingFeedReportNumber, new { @class = "form-control", id = "txtSellingFeedReportNumber", autocomplete = "off", })
